.map-wraper{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index:0;
}
map{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
}
.circle-nav{
  width: 100rpx;
  height: 100rpx;
  display: block;
  text-align: center;
  overflow: hidden;
}
.circle-nav cover-image{
  width: 100%;
  height: 100%;
}
.user-center{
  position: absolute;
  bottom: 250rpx;
  right: 30rpx;
  z-index: 10;
}
.user-msg{
  position: absolute;
  bottom: 150rpx;
  right: 30rpx;
  z-index: 10;
}
.nav-move{
  position: absolute;
  bottom: 150rpx;
  left: 30rpx;
  z-index: 10;
}
.sup-msg{
  width: 32rpx;
  height: 32rpx;
  background-color: #ff4444;
  color: #fff;
  position: absolute;
  top: 0;
  right: 0;
  border-radius: 100%;
  font-size: 18rpx;
  line-height: 32rpx;
  text-align: center;
}
.bottom-view{
  width:90%;
  position: absolute;
  bottom: 20rpx;
  left: 5%;
  text-align: center;
  z-index: 10;
  border-radius: 10rpx;
  overflow: hidden;
}
.current-address{
  width: 70%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  padding: 0 20rpx;
  box-sizing: border-box;
  border-radius: 10rpx 0 0 10rpx;
}
.search-btn{
  width: 30%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #4a4c5c;
  color: #fff;
  text-align: center;
  position: absolute;
  top: 0;
  right: 0;
  border-radius: 0 10rpx 10rpx 0;
}